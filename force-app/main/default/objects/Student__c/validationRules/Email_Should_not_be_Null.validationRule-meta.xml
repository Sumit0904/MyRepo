<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Email_Should_not_be_Null</fullName>
    <active>true</active>
    <description>Email Should not be Empty. Please fill the Email</description>
    <errorConditionFormula>OR (ISBLANK( Student_Email__c ) &amp;&amp;  ISBLANK( Description__c ))</errorConditionFormula>
    <errorMessage>Please fill the Email</errorMessage>
</ValidationRule>
